            </ul>
          </div>
        </div>
      </nav>
      <!-- End Navbar -->
      <!-- <div class="panel-header panel-header-sm">
  
  
</div> -->
      <div class="content">
        <div class="row">
          <div class="col-md-12">
            <div class="card">
              <div class="card-header">
                <h4 class="card-title"> Node 1 Parameters</h4>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                <form method = "GET" action = /user/tables>
                  <table class="table">
                    <thead class=" text-primary">
                      <th>
                        Nhiệt độ
                      </th>
                      <th>
                        Độ ẩm không khí 
                      </th>
                      <th>
                        Độ ẩm đất
                      </th>
                    </thead>
                    <% if (foo_table) {%>
				              <% for (var i = foo_table.length - 4; i < foo_table.length ; i++){%>
                    <tbody>
                      <tr>
                        <td>
                          <%= foo_table[i].nhietdo%>
                        </td>
                        <td>
                          <%= foo_table[i].doamkhongkhi%>
                        </td>
                        <td>
                          <%= foo_table[i].doamdat%>
                        </td>
                      </tr>
                    </tbody>
                      <%}%>
			              <%}%>
                  </table>
                </form>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-12">
            <div class="card card-plain">
              <div class="card-header">
                <h4 class="card-title"> Node 2 Parameters </h4>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                <form method = "GET" action = "/user/tables">
                  <table class="table">
                    <thead class=" text-primary">
                      <th>
                        Nhiệt Độ
                      </th>
                      <th>
                        Độ Ẩm
                      </th>
                      <th>
                        PH 
                      </th>
                      <th class = "text-right">
                        Oxy Hòa Tan
                      </th>
                    </tbody>
                    <% if (bar_table) {%>
				              <% for (var i = bar_table.length - 4; i < bar_table.length ; i++){%>
                    <tbody>
                      <tr>
                        <td>
                          <%= bar_table[i].nhietdo%>
                        </td>
                        <td>
                          <%= bar_table[i].doam%>
                        </td>
                        <td>
                          <%= bar_table[i].ph%>
                        </td>
                        <td class = "text-right">
                          <%= bar_table[i].oxyhoatan%>
                        </td>
                      </tr>
                    </tbody>
                      <%}%>
			              <%}%>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!--   Core JS Files   -->
