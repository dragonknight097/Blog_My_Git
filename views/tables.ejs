        <div class="simple-text logo-normal">
           <h6 style="color:SlateBlue;">Lora node</h6>
        </div>
      </div>
      <div class="sidebar-wrapper">
        <ul class="nav">
          <li>
            <a href="/index/dashboard">
              <i class="nc-icon nc-bank"></i>
              <p>Dashboard</p>
            </a>
          </li>
          <li class="active ">
            <a href="/index/tables">
              <i class="nc-icon nc-tile-56"></i>
              <p>Table List</p>
